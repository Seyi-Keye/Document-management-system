{
  "name": "document-management-system",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "buildDev": "rm -rf dist && webpack --config webpack.config.server.js && webpack --config webpack.config.dev.js",
    "buildProd": "rm -rf dist && webpack --mode production --config webpack.config.server.js && webpack --mode production --config webpack.config.prod.js",
    "build": "rm -rf dist && webpack --mode development --config webpack.config.server.js && webpack --config=webpack.config.prod.js",
    "cover": "NODE_ENV=test babel-node ./node_modules/babel-istanbul/lib/cli.js cover _mocha ./**/test/**/*.spec.* --require ./client/test/.setup.js",
    "coveralls": "nyc report --reporter=text-lcov |  coveralls",
    "e2e": "nightwatch --config nightwatch.conf.js",
    "heroku-postbuild": "rm -rf ./dist && yarn run build && cd dist/server && sequelize db:migrate",
    "seed": "NODE_ENV=test babel-node server/test/helpers/seedHelper.js",
    "start": "node ./dist/server.js",
    "start:server:dev": "nodemon server-dev.js --exec babel-node",
    "test": "npm run seed && npm run thetest",
    "test:watch": "npm run test -- --watch",
    "thetest": "NODE_ENV=test mocha ./**/test/**/*.spec.* --require ./client/test/.setup.js"
  },
